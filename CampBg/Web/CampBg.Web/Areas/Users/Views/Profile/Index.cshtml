@using Resource = CampBg.Web.Localization.Views;

@{
    ViewBag.Title = Resource.Profile;
}

<div class="row">
    <ol class="col-lg-12 breadcrumb margin-top-small">
        <li>@Html.ActionLink(Resource.Home, "Index", new { area = string.Empty, controller = "Home" })</li>
        <li class="active">@Resource.Profile</li>
    </ol>
</div>

<div>
    @Html.ActionLink(Resource.Settings, "Settings", new { controller = "Profile", area = "Users" })
</div>
<div>
    @Html.ActionLink(Resource.Delivery_addresses, "Index", new { controller = "DeliveryAdresses", area = "Users" })
</div>
<div>
    @Html.ActionLink(Resource.Order_history, "Index", new { controller = "Orders", area = "Users" })
</div>
<div>
    @Html.ActionLink(Resource.Change_password, "Manage", new { controller = "Account", area = string.Empty })
</div>