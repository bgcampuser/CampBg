@model CampBg.Web.ViewModels.ProductListMultipleViewModel

<div class="products-list-container footer-block">
    <div class="row ">
        @Html.DisplayFor(x => x.Products,
        new
            {
                @class = "col-md-6 col-lg-3 margin-top-small margin-bottom-big",
                productContainerClass = "home-page-item product-container"
            })
    </div>
</div>
<div class="col-lg-12 pull-left text-left margin-top-small">
    @for (int i = 1; i <= Model.TotalPages; i++)
    {
        <a href="#" class="page-index"> @i </a>
    }
</div>