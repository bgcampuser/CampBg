@model CampBg.Web.ViewModels.StaticPageViewModel

@{
    ViewBag.Title = Model.Title;
}

<div class="row">
    <ol class="col-lg-12 breadcrumb margin-top-small">
        <li>@Html.ActionLink("Home", "Index", new { area = string.Empty, controller = "Home" })</li>
        <li class="active">
            @string.Format("{0}", ViewBag.Title)
        </li>
    </ol>
</div>

<h2>@Model.Title</h2>

<div>@Html.Raw(Model.Content)</div>